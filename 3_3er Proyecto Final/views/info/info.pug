html
    head 
        title Merb Coder
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous")
        link(rel="stylesheet" href="./style.css")
    body
        h2(class="text-center mt-5") Información
        |
        |
        div(class="frameTable")
            div(class="frameTable2 p-3")
                table(class="table table-primary table-hover text-center")
                    thead(class="table-info")
                    tbody
                        tr
                            td(class="p-4")
                                strong Argumentos de entrada:
                            td(class="p-4 text-start")
                                each val, key in info.args
                                    div=key + ": " + val
                        tr 
                            td(class="p-4")
                                strong Nombre de la plataforma (Sistema Operativo):
                            td(class="p-4 text-start")=info.platform
                        tr 
                            td(class="p-4")
                                strong Versión de node.js:
                            td(class="p-4 text-start")=info.node
                        tr 
                            td(class="p-4")
                                strong Memoria total reservada (rss):
                            td(class="p-4 text-start")=info.rss
                        tr 
                            td(class="p-4")
                                strong Path de ejecución:
                            td(class="p-4 text-start")=info.pathEjecutable
                        tr 
                            td(class="p-4")
                                strong Process id:
                            td(class="p-4 text-start")=info.id
                        tr 
                            td(class="p-4")
                                strong Carpeta del proyecto:
                            td(class="p-4 text-start")=info.pathProyect
                        tr 
                            td(class="p-4")
                                strong Total de procesadores:
                            td(class="p-4 text-start")=info.numCPUs
                        tr 
                            td(class="p-4")
                                strong Port:
                            td(class="p-4 text-start")=info.port
                        tr 
                            td(class="p-4")
                                strong Administrador email:
                            td(class="p-4 text-start")=info.mailAdmin
        script(src="/mocks.js") 